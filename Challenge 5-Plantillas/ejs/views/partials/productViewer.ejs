<h1>Listado de productos</h1>
<div class="container">
    <% if (productsArray) { %>
        <div>
            <% productsArray.forEach(function(obj){ %>
                <h3>Nombre: <%- obj.name %></h3>
                <h4>Precio: <%- obj.price %></h4>
                <h4>Thumbnail: <%- obj.thumbnail %></h4>
            <% }); %> 
        </div>
        <div class="divider"></div>
    <% } else { %> 
        <div>
            No se encontraron productos.
        </div>
    <% } %>
</div>